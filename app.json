{
  "expo": {
    "name": "music-album-manager",
    "slug": "music-album-manager",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/images/icon-final.png",

    "scheme": "musicalbummanager",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taran5.musicalbummanager"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#E6F4FE"
      },
      "package": "com.taran5.musicalbummanager"
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "eas": {
        "projectId": "319451e5-64fb-4e3b-af91-56114775e7b6"
      }
    }
  }
}
